#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int numero, var_teste;

	numero = 7;
	setbuf(stdout, NULL);

	printf("Digite um número inteiro: "); /* prints;  */
	//scanf("%d", &numero);

	var_teste = numero%7;


	return EXIT_SUCCESS;
}
